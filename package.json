{
  "name": "gin-cms-demo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "bunx concurrently \"bun run dev:frontend\" \"bun run dev:backend\"",
    "dev:frontend": "cd frontend && bun run dev",
    "dev:backend": "cd backend && go run main.go",
    "build": "cd frontend && bun run build",
    "start": "cd frontend && bun run start",
    "lint": "cd frontend && biome check .",
    "lint:fix": "cd frontend && biome check --write .",
    "format": "cd frontend && biome format --write ."
  },
  "devDependencies": {
    "concurrently": "9.2.1"
  },
  "packageManager": "bun@1.1.0"
}
